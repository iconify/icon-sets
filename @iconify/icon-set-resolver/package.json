{
  "name": "@iconify/icon-set-resolver",
  "version": "1.1.401",
  "description": "Iconify icons collection in JSON format",
  "homepage": "https://iconify.design/icon-sets/",
  "bugs": "https://github.com/iconify/collections-json/issues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iconify/collections-json.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "readme.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts",
    "test-esm": "jest --clearCache && cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.esm.config.ts",
    "test-cjs": "yarn build && jest --clearCache && jest --config=jest.cjs.config.ts"
  },
  "dependencies": {
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.9.0",
    "@iconify/types": "^1.0.8",
    "@iconify-json/carbon": "1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "esno": "^0.10.0",
    "jest": "^27.2.2",
    "jest-each": "^27.2.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsup": "^5.2.0",
    "typescript": "^4.4.3"
  }
}
